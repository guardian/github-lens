{
  "name": "github-recently-updated",
  "version": "0.0.1",
  "description": "Creating a cache of recently updated GitHub repositories",
  "scripts": {
    "test": "echo TODO",
    "typecheck": "tsc -noEmit",
    "esbuild": "esbuild src/handler.ts --bundle --outdir=dist --platform=node --target=node18 --external:@aws-sdk/*",
    "package": "zip -FSjr dist/github-recently-updated.zip dist/handler.js",
    "build": "npm run esbuild && npm run package",
    "start": "ts-node src/run-locally.ts"
  },
  "dependencies": {
    "ts-postgres": "^1.3.1"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.363.0",
    "@aws-sdk/credential-providers": "^3.363.0",
    "@aws-sdk/rds-signer": "^3.363.0"
  }
}
