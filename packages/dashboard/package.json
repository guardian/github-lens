{
  "name": "dashboard",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test:dev": "jest --watch --onlyChanged",
    "test": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ignore-pattern playwright.config.ts --ext .ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "playwright test",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/plugin-e2e": "^1.6.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.44.1",
    "@swc/core": "1.6.5",
    "@swc/helpers": "0.5.11",
    "@swc/jest": "0.2.36",
    "@swc/register": "0.1.10",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "16.0.0",
    "@types/lodash": "^4.14.194",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.4.2",
    "identity-obj-proxy": "3.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.77.6",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/data": "11.0.0",
    "@grafana/runtime": "11.0.0",
    "@grafana/ui": "11.0.0",
    "@grafana/schema": "10.4.3",
    "@grafana/scenes": "^4.25.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.3"
  }
}
