{
  "name": "services-api",
  "version": "0.0.1",
  "description": "API for Guardian services",
  "scripts": {
    "test": "jest --detectOpenHandles --config ../../jest.config.js --selectProjects services-api",
    "prebuild": "rm -rf dist",
    "build": "esbuild src/handler.ts --bundle --outdir=dist --platform=node --target=node16 --external:aws-sdk",
    "dev": "LOCAL=true ts-node ./src/handler",
    "start": "npm run dev",
    "typecheck": "tsc -noEmit"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.5.0",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.15.13",
    "jest": "^27.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.212.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "node-fetch": "^2.6.7"
  }
}
